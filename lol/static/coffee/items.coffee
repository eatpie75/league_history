window.items=items

$(document).ready(->
	wat=(tip, el)->
		el=$(el)
		tip=$(tip)
		scroll=window.scrollY
		offset=el.offset().top-scroll
		tip.css('display', 'none').appendTo(document.body)
		[height, width]=[tip.height(), tip.width()]
		tip.remove()
		if offset-height>=0
			return 'top'
		else
			return 'bottom'
	window.connect_items=->
		$('div.item.sprite').each(->
			item=items[$(@).data('item').slice(1)]
			$(@).popover({
				content:item.description
				title:item.name
				placement:wat
				trigger:'hover'
				animation:false
			})
		)
	window.connect_items()
)
